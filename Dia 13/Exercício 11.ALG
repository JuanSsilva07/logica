Algoritmo "semnome"

Var

   i: inteiro

Inicio
   //Perfumaria
   escreval("          Múltiplos positivos de 7 inferiores a 1000 ")
   escreval("----------------------------------------------------------------")
   //Processamento de dados
   para i de 7 até <= 1000 passo 7 faca
      se (i mod 7 =0) então
         escreval(i) //Saída de dados
      fimse
   fimpara
Fimalgoritmo